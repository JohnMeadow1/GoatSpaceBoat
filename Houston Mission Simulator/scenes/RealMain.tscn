	seq.append_interval(WARNING_T)
	seq.append_callback(tr, \"hide\")
	seq.append_interval(WARNING_T)
	seq.append_callback(tr, \"show\")
	seq.append_interval(WARNING_T)
	seq.append_callback(tr, \"hide\")
	seq.append_interval(WARNING_T)
	seq.append_callback(tr, \"show\")
	seq.append_interval(WARNING_T)
	seq.append_callback(tr, \"hide\")
	seq.append_interval(WARNING_T)
	seq.append_callback(tr, \"show\")
	seq.append_interval(WARNING_T)
	seq.append_callback(tr, \"hide\")
	seq.append_interval(WARNING_T)
	seq.append_callback(tr, \"show\")
	seq.append_interval(WARNING_T)
	seq.append_callback(tr, \"queue_free\")
	
	texture = preload(\"res://Houston Mission Simulator/images/gui/ConsoleALERT.PNG\")
"

