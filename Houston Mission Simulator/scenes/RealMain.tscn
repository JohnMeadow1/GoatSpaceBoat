script/source = "extends MarginContainer

var currq: int

func _notification(what: int) -> void:
	if what == NOTIFICATION_VISIBILITY_CHANGED:
		if is_visible_in_tree():
			currq = -1
			next_q()

func next_q():
	currq += 1
	
	for i in get_child_count():
		if i == currq:
			get_child(i).show()
		else:
			get_child(i).hide()
"

